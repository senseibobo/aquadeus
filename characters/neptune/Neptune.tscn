		base_scale = 0.000001;
	}
	VELOCITY.x = -2.0*TRANSFORM[3].x;
	VELOCITY.y = -2.0*TRANSFORM[3].y;
	TRANSFORM[0].xyz *= base_scale;
	TRANSFORM[1].xyz *= base_scale;
	TRANSFORM[2].xyz *= base_scale;
	VELOCITY.z = 0.0;
	TRANSFORM[3].z = 0.0;
	if (CUSTOM.y > CUSTOM.w) {		ACTIVE = false;
	}
}

